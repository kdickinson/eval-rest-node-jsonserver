#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="C:\Users\jdd701\Documents\Personal\eval-rest-node-jsonserver\node_modules\json-server\node_modules;C:\Users\jdd701\Documents\Personal\eval-rest-node-jsonserver\node_modules;C:\Users\jdd701\Documents\Personal\node_modules;C:\Users\jdd701\Documents\node_modules;C:\Users\jdd701\node_modules;C:\Users\node_modules;C:\node_modules" "$basedir/node"  "$basedir/../json-server/bin/index.js" "$@"
  ret=$?
else 
  NODE_PATH="C:\Users\jdd701\Documents\Personal\eval-rest-node-jsonserver\node_modules\json-server\node_modules;C:\Users\jdd701\Documents\Personal\eval-rest-node-jsonserver\node_modules;C:\Users\jdd701\Documents\Personal\node_modules;C:\Users\jdd701\Documents\node_modules;C:\Users\jdd701\node_modules;C:\Users\node_modules;C:\node_modules" node  "$basedir/../json-server/bin/index.js" "$@"
  ret=$?
fi
exit $ret
